{
    "name": "tatsh/sutra",
    "type": "library",
    "description": "A set of libraries",
    "keywords": ["library"],
    "homepage": "https://github.com/tatsh/sutra",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Udvare",
            "email": "audvare@gmail.com",
            "homepage": "http://andrew.udvare.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-openssl": "*",
        "symfony/security": "dev-master",
        "symfony/filesystem": "dev-master",
        "symfony/finder": "dev-master",
        "doctrine/inflector": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "dev-master",
        "phpmd/phpmd": "dev-master",
        "sebastian/phpcpd": "dev-master",
        "squizlabs/php_codesniffer": "dev-master",
        "instaclick/symfony2-coding-standard": "dev-remaster",
        "phpdocumentor/phpdocumentor": "dev-develop",
        "raulfraile/ladybug": "dev-master",
        "symfony/process": "dev-master"
    },
    "autoload": {
        "psr-0": { "Sutra\\": "src/" }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    }
}
